{
  "name": "vue-hugo-netlify-spa-demo",
  "version": "0.0.1",
  "description": "This repo contains a demo/POC project to see how well a monorepo using Vue, Hugo, and Netlify will work.",
  "main": "index.js",
  "scripts": {
    "api-dev": "hugo serve -s ./api --disableFastRender",
    "api-build": "hugo -s ./api -d ./dist",
    "api-build-full": "hugo -s ./api -d ./dist --cleanDestinationDir",
    "api-build-preview": "hugo -s ./api -d ./dist --buildDrafts --buildFuture"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/slathrop/vue-hugo-netlify-spa-demo.git"
  },
  "keywords": [],
  "author": "SLathrop",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/slathrop/vue-hugo-netlify-spa-demo/issues"
  },
  "homepage": "https://github.com/slathrop/vue-hugo-netlify-spa-demo#readme",
  "dependencies": {
    "@luaguillande/scss_helpers": "^1.5.5",
    "vue": "^2.6.14",
    "vue-check-view": "^0.3.0"
  }
}
